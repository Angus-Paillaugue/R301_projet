{
  "scripts": {
    "format": "npx prettier . --write",
    "dev": "npm-run-all --parallel dev:*",
    "dev:styles": "npx tailwindcss -i ./styles.css -o ./app/vue/output.css --watch",
    "dev:server": "php -S localhost:8000 -t ./app/vue"
  },
  "devDependencies": {
    "@prettier/plugin-php": "^0.22.2",
    "@tailwindcss/typography": "^0.5.15",
    "npm-run-all": "^4.1.5",
    "prettier": "3.3.3",
    "tailwindcss": "^3.4.11"
  }
}
